    <validationRules>
        <fullName>Check_Follow_Up_Due_Date</fullName>
        <active>true</active>
        <description>User must fill in follow up date if the products needs to be checked by the manager</description>
        <errorConditionFormula>Is_Follow_Up_Suggested__c &amp;&amp; ISNULL(Estimated_Date__c)</errorConditionFormula>
        <errorDisplayField>Estimated_Date__c</errorDisplayField>
        <errorMessage>請填寫預計時程</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Choose_Earlier_Date</fullName>
        <active>true</active>
        <description>不讓使用者選已過的日期</description>
        <errorConditionFormula>Estimated_Date__c &lt;= TODAY()</errorConditionFormula>
        <errorDisplayField>Estimated_Date__c</errorDisplayField>
        <errorMessage>請選擇未來的日期</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Uncheck_Auto_Recommended_Products</fullName>
        <active>true</active>
        <description>不讓使用者把自動建議產品打勾取消</description>
        <errorConditionFormula>Is_Auto_Recommended__c &amp;&amp; PRIORVALUE(Is_Suggested_Product__c) != Is_Suggested_Product__c &amp;&amp; !Is_Suggested_Product__c</errorConditionFormula>
        <errorDisplayField>Is_Suggested_Product__c</errorDisplayField>
        <errorMessage>無法打勾取消自動建議的產品</errorMessage>
    </validationRules>
</CustomObject>